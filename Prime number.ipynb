    "def is_prime(n):\n",
    "    if n < 2:\n",
    "        return False\n",
    "    elif n == 2:\n",
    "        return True\n",
    "    else:\n",
    "        for i in range(2, int(n**(0.5) + 1)):\n",
    "            if n % i == 0:\n",
    "                return False\n",
    "        return True"
   ]
